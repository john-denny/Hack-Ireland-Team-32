{
    "manifest_version": 3,
    "name": "Receipt AutoFill",
    "version": "1.0",
    "permissions": ["storage", "tabs", "scripting"],
    "host_permissions": ["https://www.ros.ie/*"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    }
  }
  